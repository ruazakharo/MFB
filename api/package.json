    "@types/jsonwebtoken": "7.2.6",
    "@types/lodash": "4.14.108",
    "@types/mongodb": "3.0.15",
    "@types/swaggerize-express": "4.0.32",
    "compression": "1.7.2",
    "config": "1.30.0",
    "express": "4.16.3",
    "fs-extra": "5.0.0",
    "http-status-codes": "1.3.0",
    "js-yaml": "3.11.0",
    "jsonwebtoken": "8.2.1",
    "lodash": "4.17.10",
    "log4js": "2.5.3",
